{
    "name":        "josantonius/database",
    "version":     "1.1.0",
    "type":        "library",
    "description": "Library for SQL database management to be used by several providers at the same time.",
    "keywords": [
        "Database",
        "MySQL",
        "MariaDB",
        "SQL",
        "Query",
        "HHVM",
        "ORM",        
        "PHP"
    ],
    "license": "MIT",
    "authors": [
        {
            "name":     "Josantonius",
            "email":    "info@josantonius.com",
            "homepage": "https://github.com/josantonius",
            "role":     "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php" : ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "Josantonius\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Josantonius\\Database\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
